{
  "name": "react-sketch-canvas",
  "version": "3.0.0",
  "description": "react-sketch-canvas - Freehand vector drawing tool for React using SVG as canvas",
  "source": "src/index.ts",
  "main": "dist/react-sketch-canvas.js",
  "umd:main": "dist/react-sketch-canvas.umd.js",
  "module": "dist/react-sketch-canvas.m.js",
  "unpkg": "dist/react-sketch-canvas.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && microbundle  --jsx React.createElement --globals react=React",
    "clean": "rimraf dist",
    "start": "yarn clean && microbundle watch  --jsx React.createElement --globals react=React"
  },
  "dependencies": {
    "immer": "^6.0.3"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@types/immutable": "^3.8.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "lerna": "^3.20.2",
    "microbundle": "^0.12.0-next.8",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "author": "Vinoth Pandian",
  "homepage": "https://github.com/vinothpandian/react-sketch-canvas",
  "license": "MIT",
  "repository": "https://github.com/vinothpandian/react-sketch-canvas.git",
  "keywords": [
    "react-component",
    "sketch",
